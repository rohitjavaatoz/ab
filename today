import org.springframework.stereotype.Service;
import java.util.ArrayList;
import java.util.List;

@Service
public class MappingService {

    public List<EmployeeDTO> mapToDTO(List<Employee> employees) {
        List<EmployeeDTO> dtos = new ArrayList<>();

        for (Employee employee : employees) {
            List<TabDTO> tabDTOs = new ArrayList<>();

            for (Tab tab : employee.getTabs()) {
                List<AttributeDTO> attributeDTOs = new ArrayList<>();

                for (Attribute attribute : tab.getAttributes()) {
                    AttributeDTO attributeDTO = new AttributeDTO();
                    attributeDTO.setName(attribute.getName());
                    attributeDTO.setMaintenanceType(attribute.getMaintenanceType());
                    attributeDTO.setCicImpacted(attribute.getCicImpacted());
                    // Add other fields if needed

                    attributeDTOs.add(attributeDTO);
                }

                TabDTO tabDTO = new TabDTO();
                tabDTO.setName(tab.getName());
                tabDTO.setAttributes(attributeDTOs);

                tabDTOs.add(tabDTO);
            }

            EmployeeDTO dto = new EmployeeDTO();
            dto.setTabs(tabDTOs);

            dtos.add(dto);
        }

        return dtos;
    }
}
-----------------
import org.springframework.stereotype.Service;
import java.util.List;
import java.util.stream.Collectors;

@Service
public class MappingService {

    public List<EmployeeDTO> mapToDTO(List<Employee> employees) {
        return employees.stream()
                .map(this::mapEmployeeToDTO)
                .collect(Collectors.toList());
    }

    private EmployeeDTO mapEmployeeToDTO(Employee employee) {
        List<TabDTO> tabDTOs = employee.getTabs().stream()
                .map(this::mapTabToDTO)
                .collect(Collectors.toList());

        EmployeeDTO employeeDTO = new EmployeeDTO();
        employeeDTO.setTabs(tabDTOs);

        return employeeDTO;
    }

    private TabDTO mapTabToDTO(Tab tab) {
        List<AttributeDTO> attributeDTOs = tab.getAttributes().stream()
                .map(this::mapAttributeToDTO)
                .collect(Collectors.toList());

        TabDTO tabDTO = new TabDTO();
        tabDTO.setName(tab.getName());
        tabDTO.setAttributes(attributeDTOs);

        return tabDTO;
    }

    private AttributeDTO mapAttributeToDTO(Attribute attribute) {
        AttributeDTO attributeDTO = new AttributeDTO();
        attributeDTO.setName(attribute.getName());
        attributeDTO.setMaintenanceType(attribute.getMaintenanceType());
        attributeDTO.setCicImpacted(attribute.getCicImpacted());
        // Add other fields if needed

        return attributeDTO;
    }
}
---------------------
import org.springframework.stereotype.Service;
import java.util.List;
import java.util.stream.Collectors;

@Service
public class MappingService {

    public List<EmployeeDTO> mapToDTO(List<Employee> employees) {
        return employees.stream()
                .map(employee -> {
                    EmployeeDTO employeeDTO = new EmployeeDTO();
                    employeeDTO.setTabs(
                            employee.getTabs().stream()
                                    .map(tab -> {
                                        TabDTO tabDTO = new TabDTO();
                                        tabDTO.setName(tab.getName());
                                        tabDTO.setAttributes(
                                                tab.getAttributes().stream()
                                                        .map(attribute -> {
                                                            AttributeDTO attributeDTO = new AttributeDTO();
                                                            attributeDTO.setName(attribute.getName());
                                                            attributeDTO.setMaintenanceType(attribute.getMaintenanceType());
                                                            attributeDTO.setCicImpacted(attribute.getCicImpacted());
                                                            
                                                            // Handle null values
                                                            if (attributeDTO.getMaintenanceType() == null) {
                                                                attributeDTO.setMaintenanceType("DEFAULT"); // Example default value
                                                            }
                                                            if (attributeDTO.getCicImpacted() == null) {
                                                                attributeDTO.setCicImpacted(false); // Example default value
                                                            }
                                                            
                                                            // Add other fields if needed
                                                            return attributeDTO;
                                                        })
                                                        .collect(Collectors.toList())
                                        );
                                        return tabDTO;
                                    })
                                    .collect(Collectors.toList())
                    );
                    return employeeDTO;
                })
                .collect(Collectors.toList());
    }
}

