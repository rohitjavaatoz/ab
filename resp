import lombok.Data;

@Data
public class AttributeDTO {
    private String name;
    private String maintenanceType;
    private boolean cicImpacted;
}
---
import java.util.List;
import lombok.Data;

@Data
public class TabDTO {
    private String name;
    private List<AttributeDTO> attributes;
}
----
import java.util.List;
import lombok.Data;

@Data
public class ResponsePayloadDTO {
    private List<TabDTO> tabs;
}

